<%- include("partials/header"); -%>

<h1>Home</h1>
<p> <%= homeStartingContent %> </p>

<% posts.forEach(function(post){ %>
    <h1> <%= post.title %> </h1>

    <% if(post.body.length > 3){ %>
        <p> <%= post.body.substring(0,3) + "..." %> </p>
    <% } else { %>
        <p> <%= post.body %> </p>
    <% } %>

<% }) %>

<%- include("partials/footer"); -%>